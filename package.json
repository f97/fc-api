{
  "name": "fc-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2"
  },
  "eslintConfig": {
      "extends": "airbnb-base",
      "env": {
          "es6": true,
          "browser": true
      },
      "rules": {
          "brace-style": [
              "error",
              "stroustrup"
          ],
          "comma-dangle": [
              "error",
              "never"
          ],
          "no-unused-vars": [
              "warn"
          ]
      }
  }
}
